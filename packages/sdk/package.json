{
  "name": "@notionpet/sdk",
  "title": "@notionpet/sdk",
  "keywords": [
    "notion",
    "notionpet",
    "@notion",
    "@notionpet",
    "notion sdk"
  ],
  "version": "0.1.6",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/kuai-dian/notion-pet/tree/master/packages/sdk",
  "description": "@notionpet/sdk 是开发组件用的工具包, 结合 Notion.pet 平台，开发好后的组件可以发布到该平台然后通过 embed 嵌入notion软件中.",
  "files": [
    "dist"
  ],
  "main": "./dist/@notionpet/sdk.umd.js",
  "module": "./dist/@notionpet/sdk.es.js",
  "types": "./dist/sdk.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@notionpet/sdk.es.js",
      "require": "./dist/@notionpet/sdk.umd.js",
      "types": "./dist/sdk.d.ts"
    },
    "./dist/*": {
      "import": "./dist/@notionpet/sdk.es.js",
      "require": "./dist/@notionpet/sdk.umd.js",
      "types": "./dist/sdk.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build",
    "build": "vite build",
    "start": "vite build --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "胡俊平",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.9",
    "eslint": "^8.9.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.12",
    "vite-plugin-dts": "^0.9.9"
  },
  "gitHead": "69514ae7bd6a258930f3b2a6df7dfcc5cf283bd9"
}
